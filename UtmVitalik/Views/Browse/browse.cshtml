@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet">

    <title>Stun Seed - Awesome Store</title>

    <!-- Bootstrap core CSS -->
    <link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <!-- Additional CSS Files -->
    <link rel="stylesheet" href="../../Vendor/fontawesome.css">
    <link rel="stylesheet" href="../../Vendor/stun seed.css">
    <link rel="stylesheet" href="../../Vendor/owl.css">
    <link rel="stylesheet" href="../../Vendor/animate.css">
    <link rel="stylesheet" href="https://unpkg.com/swiper@7/swiper-bundle.min.css"/>
</head>

<body>

<!-- ***** Preloader Start ***** -->
<div id="js-preloader" class="js-preloader">
    <div class="preloader-inner">
        <span class="dot"></span>
        <div class="dots">
            <span></span>
            <span></span>
            <span></span>
        </div>
    </div>
</div>
<!-- ***** Preloader End ***** -->

<!-- ***** Header Area Start ***** -->
<header class="header-area header-sticky">
    <div class="container">
        <div class="row">
            <div class="col-12">
                <nav class="main-nav">
                    <!-- ***** Logo Start ***** -->
                    <a href="index.cshtml" class="logo">
                        <img src="Content/Images/logo.png" alt="">
                    </a>
                    <!-- ***** Logo End ***** -->
                    <!-- ***** Search End ***** -->
                    <div class="search-input">
                        <form id="search" action="#">
                            <input type="text" placeholder="Type Something" id='searchText' name="searchKeyword" onkeypress="handle"/>
                            <i class="fa fa-search"></i>
                        </form>
                    </div>
                    <!-- ***** Search End ***** -->
                    <!-- ***** Menu Start ***** -->
                    <ul class="nav">
                        <li>
                           @Html.ActionLink("Home", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
                        </li>
                        <li>
                            <a href="browse.cshtml" class="active">Browse</a>
                        </li>
                        <li>
                            <a href="details.cshtml">Details</a>
                        </li>
                        <li>
                            <a href="profile.cshtml">Profile <img src="Content/Images/profile-header.jpg" alt=""></a>
                        </li>
                    </ul>
                    <a class='menu-trigger'>
                        <span>Menu</span>
                    </a>
                    <!-- ***** Menu End ***** -->
                </nav>
            </div>
        </div>
    </div>
</header>
<!-- ***** Header Area End ***** -->

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="page-content">

                <!-- ***** Featured Games Start ***** -->
                <div class="row">
                    <div class="col-lg-8">
                        <div class="featured-games header-text">
                            <div class="heading-section">
                                <h4><em>Featured</em> Games</h4>
                            </div>
                            <div class="owl-features owl-carousel">

                            </div>
                            }
                        </div>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="top-downloaded">
                        <div class="heading-section">
                            <h4><em>Top</em> Downloaded</h4>
                        </div>
                        <ul>

                        </ul>
                        <div class="text-button">
                            <a href="profile.cshtml">View All Games</a>
                        </div>
                    </div>
                </div>
            </div>
            <!-- ***** Featured Games End ***** -->

        </div>
    </div>
</div>

<footer>
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <p>Copyright © 2024 <a href="#">Stun Seed</a> Company. All rights reserved.

            </div>
        </div>
    </div>
</footer>

<!-- Scripts -->
<!-- Bootstrap core JavaScript -->
<script src="../../Vendor/Scripts/jquery.min.js"></script>
<script src="../../Vendor/Scripts/bootstrap.min.js"></script>

<script src="../../Vendor/Scripts/isotope.min.js"></script>
<script src="../../Vendor/Scripts/owl-carousel.js"></script>
<script src="../../Vendor/Scripts/tabs.js"></script>
<script src="../../Vendor/Scripts/popup.js"></script>
<script src="../../Vendor/Scripts/custom.js"></script>
</body>

</html>